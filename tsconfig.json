{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "cxApi/*":["src/api/*"],
      "cxAsset/*":["src/assets/*"],
      "cxComponent/*":["src/components/*"],
      "cxLayout/*":["src/layouts/*"],
      "cxLocale/*":["src/locales/*"],
      "cxPage/*":["src/pages/*"],
      "cxUtil/*":["src/utils/*"],
      "~":[""]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
